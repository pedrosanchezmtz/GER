                  outputVariable="BpelUpdateInvoicesInstallmentsAPRs"/>
          <assign name="Transformation2">
            <bpelx:annotation>
              <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
            </bpelx:annotation>
            <copy>
              <from>ora:doXSLTransformForDoc("../Transformations/Trs_BpelUpdateSuppliersInvoicesAPRs.xsl", $outputVariable.payload, "BpelUpdateInvoicesInstallmentsAPRs.payload", $BpelUpdateInvoicesInstallmentsAPRs.payload)</from>
