  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in BpelUpdateSupplierInvoicesInstallmentsAP.wsdl) --><receive name="receiveInput" partnerLink="bpelupdatesupplierinvoicesinstallmentsap_client" portType="client:BpelUpdateSupplierInvoicesInstallmentsAP" operation="UpdateSupplierInvoicesInstallmentsAP" variable="inputVariable" createInstance="yes"/>
    <forEach parallel="no" counterName="countSuppliers" name="ForEach_Suppliers">
      <startCounterValue>1</startCounterValue>
      <finalCounterValue>count($inputVariable.payload/ns4:Supplier)</finalCounterValue>
      <scope name="ScopeSupplier">
        <variables>
          <variable name="v_paymentsInvoices" element="ns4:PaymentsInvoices"/>
          <variable name="BpelUpdateInvoicesInstallmentsAPRq"
                    messageType="ns2:BpelUpdateInvoicesInstallmentsAPRequestMessage"/>
          <variable name="BpelUpdateInvoicesInstallmentsAPRs"
                    messageType="ns2:BpelUpdateInvoicesInstallmentsAPResponseMessage"/>
        </variables>
        <sequence name="SeqSupplier">
          <scope name="ScopeGetPaymentAPOLO" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
            <variables>
              <variable name="v_stringXML" type="xsd:string"/>
              <variable name="WsErpIntegrationBizGenerateReportRq" messageType="ns3:GenerateReportRq"/>
              <variable name="WsErpIntegrationBizGenerateReportRs" messageType="ns3:GenerateReportRs"/>
            </variables>
            <sequence name="SeqGetPaymentAPOLO">
              <assign name="assignParams" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                      xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                  <from>dvm:lookupValue("DVMs/dvmValidationPaymentsBizConfigs.dvm","Code","TargetInterface","Value","","Component","ValidationPaymentsBiz")</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:TargetInterface</to>
                </copy>
                <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                      xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                  <from>ora:getCompositeName()</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:IntegrationName</to>
                </copy>
                <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                      xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                  <from>ora:getComponentName()</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:ProcessName</to>
                </copy>
                <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                      xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                  <from>dvm:lookupValue("DVMs/dvmValidationPaymentsBizConfigs.dvm","Code","ReportName","Value","","Component","GetPaymentsInvoices")</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:ReportName</to>
                </copy>
                <copy bpelx:insertMissingToData="yes" xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
                      ignoreMissingFromData="yes">
                  <from>'p_rfc_proveedor'</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:parameterNameValues/ns3:item[1]/ns3:name</to>
                </copy>
                <copy bpelx:insertMissingToData="yes" xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
                      ignoreMissingFromData="yes">
                  <from>$inputVariable.payload/ns4:Supplier[$countSuppliers]/ns4:supplier_rfc</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:parameterNameValues/ns3:item[1]/ns3:values/ns3:item[1]</to>
                </copy>
                <copy bpelx:insertMissingToData="yes" xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
                      ignoreMissingFromData="yes">
                  <from>'p_num_proveedor'</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:parameterNameValues/ns3:item[2]/ns3:name</to>
                </copy>
                <copy bpelx:insertMissingToData="yes" xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
                      ignoreMissingFromData="yes">
                  <from>$inputVariable.payload/ns4:Supplier[$countSuppliers]/ns4:supplier_number</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:parameterNameValues/ns3:item[2]/ns3:values/ns3:item[1]</to>
                </copy>
              </assign>
              <invoke name="WsErpIntegrationBiz" partnerLink="WsERPIntegrationBiz"
                      portType="ns3:ERPIntegrationBizPortType" operation="GenerateReport"
                      inputVariable="WsErpIntegrationBizGenerateReportRq"
                      outputVariable="WsErpIntegrationBizGenerateReportRs"
                      xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
                      xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable" bpelx:invokeAsDetail="no"/>
              <assign name="AssignParseStringXML" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                <copy ignoreMissingFromData="yes">
                  <from>concat('&lt;DATA_DS xmlns="http://soa.estrellaroja.com.mx/ValidationPaymentsAPBiz"&gt;',substring-after($WsErpIntegrationBizGenerateReportRs.GenerateReportRs/ns3:Return/ns3:xmlOut,'&lt;DATA_DS&gt;'))</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$v_stringXML</to>
                </copy>
                <copy ignoreMissingFromData="yes">
                  <from>oraext:parseXML($v_stringXML)</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$v_paymentsInvoices/ns4:DATA_DS</to>
                </copy>
              </assign>
            </sequence>
          </scope>
          <if name="If_Valida">
            <documentation>
              <![CDATA[SegirRetener]]>
            </documentation>
            <condition>$inputVariable.payload/ns4:Supplier/ns4:validate=true( ) and count($v_paymentsInvoices/ns4:DATA_DS/ns4:SUPPLIER/ns4:PAYMENT)&gt;0 and $inputVariable.payload/ns4:Supplier/ns6:HoldFlag='true'</condition>
            <assign name="AssignSeguirReteniendo">
              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                <from>'true'</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$inputVariable.payload/ns4:Supplier/ns6:HoldFlag</to>
              </copy>
              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                <from>'Pago no valido'</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$inputVariable.payload/ns4:Supplier/ns6:HoldReason</to>
              </copy>
            </assign>
          </if>
          <assign name="XQTrs_UpdateInvoicesInstallmentsAPRq">
            <bpelx:annotation>
              <bpelx:pattern patternName="bpelx:xquery"></bpelx:pattern>
            </bpelx:annotation>
            <copy>
              <from>ora:processXQuery10('../Transformations/XQTrs_UpdateInvoicesInstallmentsAPRq.xqy', 'inputVariable.payload', $inputVariable.payload, 'v_paymentsInvoices', $v_paymentsInvoices)</from>
