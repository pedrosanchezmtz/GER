    <partnerLink name="bpelupdatesupplierinvoicesinstallmentsap_client" partnerLinkType="client:BpelUpdateSupplierInvoicesInstallmentsAP" myRole="BpelUpdateSupplierInvoicesInstallmentsAPProvider"/>
    <partnerLink name="WsERPIntegrationBiz" partnerLinkType="ns3:WsERPIntegrationBiz"
