									<attribute name="parametro" type="string"/>
									<attribute name="metodo" type="string"/>
									<attribute name="resultado" type="string"/>
									<attribute name="especificacionMin" type="string"/>
									<attribute name="especificacionMax" type="string"/>
								</complexType>
							</element>
							<element name="microbiologico" maxOccurs="unbounded">
								<complexType>
									<attribute name="parametro" type="string"/>
									<attribute name="metodo" type="string"/>
									<attribute name="resultado" type="string"/>
									<attribute name="especificacionMin" type="string"/>
									<attribute name="especificacionMax" type="string"/>
								</complexType>
							</element>

							<element name="analisisProducto" maxOccurs="unbounded">
