<?xml version='1.0' encoding='UTF-8'?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

	<element name="generatePDFQAResponse">
		<complexType>
			<sequence>
				<element name="pdfBase64" type="base64Binary"/>
