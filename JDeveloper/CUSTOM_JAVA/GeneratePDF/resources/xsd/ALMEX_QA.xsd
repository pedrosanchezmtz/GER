								</complexType>
							</element>
							<element name="defectosMenores" maxOccurs="unbounded">
								<complexType>
									<attribute name="defectosMenores" type="string"/>
									<attribute name="num" type="integer"/>
								</complexType>
							</element>
							<element name="defectosCriticos" maxOccurs="unbounded">
								<complexType>
									<attribute name="defectosCriticos" type="string"/>
									<attribute name="num" type="integer"/>
								</complexType>
							</element>
						</sequence>
						<attribute name="folioCertificado" type="string"/>
						<attribute name="presentacion" type="string"/>
